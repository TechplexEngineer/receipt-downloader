{
  "compilerOptions": {
    "target": "ES2017",
    "module": "ES2022",
    "lib": [
      "es6",
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "declaration": true,
    "outDir": "lib",
    "strict": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "test/"
  ]
}