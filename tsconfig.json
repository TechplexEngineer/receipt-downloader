{
  "compilerOptions": {
    "target": "ES2015",
    "module": "ES2015",
    "lib": [
      "es6"
    ],
    "declaration": true,
    "outDir": "lib",
    "strict": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "test/"
  ]
}